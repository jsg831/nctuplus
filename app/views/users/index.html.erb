<div class="container">
  <h1>使用者列表</h1>
  <table class="table table-hover">
    <thead>
      <tr>
        <th>使用者ID</th>
        <th>使用者名稱</th>
      </tr>
    </thead>
    </tbody>
      <% @users.each do |user| %>
        <tr>
          <td class="col-xs-1"><%= link_to user.user_id, user %></td>
          <td class="col-xs-1"><%= user.name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
